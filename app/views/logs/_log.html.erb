<div class="card">
  <div class="card-header" id="heading<%= log.id.to_s %>">
    <h5 class="mb-0">
      <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse<%= log.id.to_s %>" aria-expanded="false" aria-controls="collapse<%= log.id.to_s %>">
        Alerta desde <%= log.ip_log %>
      </button>
    </h5>
  </div>

  <div id="collapse<%= log.id.to_s %>" class="collapse" aria-labelledby="heading<%= log.id.to_s %>" data-parent="#accordion">
    <div class="card-body">
      Injeccion detectada: <%= log.tipo_ataque %>
    </div>
  </div>
</div>
